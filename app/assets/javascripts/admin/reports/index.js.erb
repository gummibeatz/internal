//=require ./report.js

var report;

$(document).ready(function() {
  report = new Report();
  $("form").bind('submit', function(e) {
    e.preventDefault();

    var startVal = $(this).find('#start-date').val();
    var endVal = $(this).find('#end-date').val();

    report.fetch({
      start: DateHelper.formatParsable(startVal),
      end: DateHelper.formatParsable(endVal)
    }, function() {
      console.log(report);
    });
  });
});

<div class="container">

  <div class="row">
    <% @cohorts.each do |cohort| %>
      <div class="col-md-6">
        <h2><%= "#{cohort.name} #{cohort.version}" %></h2>
        <% cohort.developers.each do |developer| %>
          <p><%= link_to developer.full_name, developer_path(developer) %></p>
        <% end %>
      </div>
    <% end %>
  </div>
</div>

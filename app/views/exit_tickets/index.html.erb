<div class="container">
  <div class="row">
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          Import exit tickets
        </div>
        <div class="panel-body">
          <%= form_tag upload_exit_tickets_path, multipart: true do %>
            <%= file_field_tag :file, class: "pull-left" %>
            <%= submit_tag "Import", class: "pull-left" %>
          <% end %>
        </div>
      </div>
    <div class="col-md-12">
	
    </div>
  </div>
</div>

<!--div for d3 chart, added by Veena 20150808 -->
<div id="chart">
	<script src="/app/assets/javascripts/exit_tickets/exit_ticket_graphs.js">
	</script>
</div>

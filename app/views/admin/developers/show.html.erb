<div class="container">
  <div class="row">
    <div class="col-md-12">
      <%= render partial: "/shared/modules/editable_header", locals: {title: developer.display_name, edit_url: edit_developer_path(developer)} %>
    </div>

    <div class="col-xs-12">
      <h3 data-toggle="collapse" data-target="#admin-dev-assessments-panel" aria-expanded="false" aria-controls="admin-dev-assessments-panel">
        Assessments
      </h3>
      <div id="admin-dev-assessments-panel" class="collapse in">
      </div>
    </div>

    <div class="col-xs-12">
      <h3 data-toggle="collapse" data-target="#admin-dev-attendances-panel" aria-expanded="false" aria-controls="admin-dev-attendances-panel">
        Attendance
      </h3>
      <div id="admin-dev-attendances-panel" class="collapse">
      </div>
    </div>

    <div class="col-md-12">
      <h3 data-toggle="collapse" data-target="#developer-basic-info" aria-expanded="false" aria-controls="developer-basic-info">
        Basic info
      </h3>
      <div id="developer-basic-info" class="collapse">
        <% developer.attributes.each_pair do |name, value| %>
          <% next if name == "id" %>
          <% next if name == "application_id" %>
          <p><strong><%= "#{name}:" %></strong>  <%= value %></p>
        <% end %>
      </div>
    </div>

    <div class="col-md-12">
      <h3 data-toggle="collapse" data-target="#developer-exit-tickets" aria-expanded="false" aria-controls="developer-exit-tickets">
        Exit Tickets
      </h3>
      <div id="developer-exit-tickets" class="collapse">
        <%= render "/admin/exit_tickets/form_table", tickets: developer.exit_tickets, link: true, comments: true %>
      </div>
    </div>
  </div>
</div>

<script>
window.developerID = <%= developer.id %>;
</script>
